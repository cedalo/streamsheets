{
  "name": "@cedalo/service-core",
  "version": "1.4.0",
  "description": "A package providing basic service classes.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/jest --runInBand --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedalo/streamsheets.git"
  },
  "author": "Cedalo AG",
  "bugs": {
    "url": "https://github.com/cedalo/streamsheets/issues"
  },
  "homepage": "https://github.com/cedalo/streamsheets/tree/master/packages/service-core",
  "dependencies": {
    "@cedalo/error-codes": "*",
    "@cedalo/id-generator": "*",
    "@cedalo/logger": "*",
    "@cedalo/messaging-client": "*",
    "@cedalo/protocols": "*",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "cors": "2.8.4",
    "express": "4.16.4",
    "http-errors": "1.6.1",
    "mkdirp": "0.5.1",
    "morgan": "1.9.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "eslint": "4.18.2",
    "eslint-config-cedalo": "git+ssh://git@github.com/cedalo/eslint-config-cedalo.git",
    "eslint-config-prettier": "3.3.0",
    "jest": "20.0.0",
    "jest-cli": "18.1.0"
  },
  "eslintConfig": {
    "extends": [
      "cedalo",
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "global-require": 1,
      "no-undef": 1,
      "no-unused-vars": 1
    }
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "license": "EPL-2.0"
}
