{
  "name": "@cedalo/util",
  "version": "1.4.0",
  "description": "Util package",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/jest --runInBand --forceExit ./test/"
  },
  "keywords": [],
  "author": "Cedalo AG",
  "homepage": "https://github.com/cedalo/streamsheets/tree/master/packages/util",
  "devDependencies": {
    "eslint": "4.18.2",
    "eslint-config-cedalo": "git+ssh://git@github.com/cedalo/eslint-config-cedalo.git",
    "eslint-plugin-import": "2.8.0",
    "eslint-config-prettier": "3.3.0",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "sinon": "4.1.3"
  },
  "dependencies": {},
  "eslintConfig": {
    "extends": [
      "cedalo",
      "prettier"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleDirectories": [
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 90,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "bugs": {
    "url": "https://github.com/cedalo/streamsheets/issues"
  },
  "repository": {
    "url": "git+https://github.com/cedalo/streamsheets.git"
  },
  "license": "EPL-2.0"
}
