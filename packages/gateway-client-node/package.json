{
  "name": "@cedalo/gateway-client-node",
  "version": "1.4.0",
  "description": "Node.js client for the gateway",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/jest --runInBand --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedalo/streamsheets.git"
  },
  "author": "Cedalo AG",
  "bugs": {
    "url": "https://github.com/cedalo/streamsheets/issues"
  },
  "homepage": "https://github.com/cedalo/streamsheets/tree/master/packages/gateway-client-node",
  "dependencies": {
    "@cedalo/gateway-client": "*",
    "@cedalo/protocols": "*",
    "ws": "7.1.0"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-preset-env": "1.6.0",
    "eslint": "4.18.2",
    "eslint-config-cedalo": "git+ssh://git@github.com/cedalo/eslint-config-cedalo.git",
    "eslint-config-prettier": "3.3.0",
    "jasmine-core": "2.6.4",
    "jest": "20.0.0",
    "jest-cli": "18.1.0",
    "@cedalo/jsg-core": "*",
    "karma": "3.1.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-opera-launcher": "1.0.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-safari-launcher": "1.0.0",
    "sinon": "7.2.2"
  },
  "eslintConfig": {
    "extends": [
      "cedalo",
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "global-require": 1,
      "no-undef": 1,
      "no-unused-vars": 1
    }
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "license": "EPL-2.0"
}
